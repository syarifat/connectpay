{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.2" }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/storage/(.*)",
            "dest": "/public/storage/$1"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/public/assets/$1"
        },
        {
            "src": "/uploads/(.*)",
            "dest": "/public/uploads/$1"
        },
        {
            "src": "/logo.png",
            "dest": "/public/logo.png"
        },
        {
            "src": "/(.*).(png|jpg|jpeg|gif|svg|ico|css|js)",
            "dest": "/public/$1.$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}